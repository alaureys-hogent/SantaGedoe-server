{
  "name": "santagedoe",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npx env-cmd nodemon",
    "web": "node src/index.js",
    "test": "npx env-cmd -f .env.test jest",
    "test:coverage": "npx env-cmd -f .env.test jest --coverage",
    "test:ci": "jest",
    "lint": "npx eslint . --fix"
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "argon2": "^0.29.1",
    "config": "^3.3.6",
    "env-cmd": "^10.1.0",
    "jest": "^27.3.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa2-swagger-ui": "^5.6.0",
    "mysql2": "^2.3.0",
    "node-emoji": "^1.11.0",
    "serialize-error": "8.1.0",
    "supertest": "^6.1.6",
    "swagger-jsdoc": "^6.2.5",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "eslint-plugin-import": "^2.26.0",
    "nodemon": "^2.0.19"
  }
}
